{
  "name": "cypress_automation_repo2",
  "version": "1.0.0",
  "description": "Automation using cypress for JET",
  "main": "index.js",
  "scripts": {
    "cversion": "cypress --version",
    "test": "cypress run",
    "test:ci": "NO_COLOR=1 cypress run",
    "openCypressRunner": "node_modules/.bin/cypress open",
    "E2E_Place_Order_Single": "npm run test -- --reporter mochawesome --spec cypress/integration/TestCases_All/E2E_Place_Order_Single.js",
    "E2E_Place_Order_Standard": "npm run test -- --reporter mochawesome --spec cypress/integration/TestCases_All/E2E_Place_Order_Standard.js",
    "merge": "mochawesome-merge cypress/reports/*.json > cypress/reports/output.json",
    "generate_mochawesome_report": "merge cypress/reports/output.json --reportDir ./ --inline",
    "final_report": "npm run merge && npm run generate_mochawesome_report"
  },
  "keywords": [
    "work_tejas1 JET AUTOMATION"
  ],
  "author": "work_tejas1",
  "license": "ISC",
  "devDependencies": {
    "chai": "^4.3.4",
    "clipboardy": "^2.3.0",
    "cypress": "^8.6.0",
    "cypress-dark": "^1.8.0",
    "cypress-wait-until": "^1.7.1",
    "cypress-xpath": "^1.6.2",
    "mochawesome": "^6.2.2",
    "mochawesome-merge": "^4.2.0",
    "mochawesome-report-generator": "^5.2.0"
  },
  "dependencies": {
    "mocha": "^8.4.0"
  }
}
